{
  "compilerOptions": {
    "esModuleInterop": true,  // import를 사용하기 위해
    "jsx": "react",  // tsx 파일을 사용하기 위해
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
  },
  "exclude": [
    "node_modules"
  ]
}